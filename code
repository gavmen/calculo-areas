x = input().split()
valor1, valor2, valor3 = x
a = float(valor1)
b = float(valor2)
c = float(valor3)
tr = (a * c) / 2
circ = 3.14159 * (c * c)
trp = (a + b) * c / 2
qrd = b * b
rt = a * b
print('TRIANGULO: ' "%.3f" % tr)
print('CIRCULO: ' "%.3f" % circ)
print('TRAPEZIO: ' "%.3f" % trp)
print('QUADRADO: ' "%.3f" % qrd)
print('RETANGULO: ' "%.3f" % rt)
